{
    "manifest_version": 2,
    "name": "Leetcode Link Opener",
    "description": "Open leetcode links one by one",
    "version": "1.0.0",
    "icons": {
      "128": "icon128.png"
    },
    "browser_action": {
      "default_icon": "icon16.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "storage",
      "scripting",
      "unlimitedStorage",
      "<all_urls>"
    ]
  }
  